{
  "name": "rustalytics",
  "version": "1.0.0",
  "description": "Rustalytics - World's fastest, simplest and cheapest analytics",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "analytics",
    "events",
    "tracking",
    "rustalytics"
  ],
  "author": "William Espegren",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.6",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.36.1",
    "globals": "^15.9.0",
    "typescript": "^4.5.4",
    "typescript-eslint": "^8.6.0",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.31.2"
  },
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
